
        
        
        local playerName = game.Players.LocalPlayer.Name
            local WorkSpace = game:GetService("Workspace")
            local Workspaceplayer = WorkSpace:FindFirstChild(playerName)
     if WorkSpace:FindFirstChild(playerName) then
        while true do
            wait(0.1)
            if Workspaceplayer:FindFirstChild("Musket") then
        
   Workspaceplayer.Musket.Loaded.Value = true
       Workspaceplayer.Musket.Ammo.Value = 1
        Workspaceplayer.Musket.MadeReady.Value = true
        Workspaceplayer.Musket.LDS.Value = true
        Workspaceplayer.Musket.Aiming.Value = true
            end

        end
    end
